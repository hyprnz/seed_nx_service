{
    "name": "@hyprnz/source",
    "version": "0.0.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "pre-commit": "lint-staged",
        "docker:dev": "docker compose -f .devcontainer/docker-compose.yml",
        "docker:dev:connect": "npm run docker:dev -- exec hyprnz-app bash",
        "docker:dev:down": "npm run docker:dev -- down",
        "docker:dev:logs": "npm run docker:dev -- logs -f",
        "docker:dev:up": "npm run docker:dev -- up -d"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --quiet --fix"
        ],
        "*.{json,js,ts,jsx,tsx,html}": [
            "prettier --write --ignore-unknown"
        ]
    },
